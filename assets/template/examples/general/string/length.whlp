#project "String charCode example"

#include "lib/modules/standard.whl"
#include "lib/modules/string.whl"

#display "Console"

string fruit[5] = ["Banana", "Apple", "Orange", "Pear", "Melon"]

proc main()
    number n
    for n = 0 to 4
        printS(fruit[n])
        printN(length(fruit[n]))
    end

    halt()
end

