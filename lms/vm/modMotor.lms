    DATA8  lBitfield_8

    PORT_CNV_INPUT(pPort_32, pLayer_8, lBitfield_8)
    OUTPUT_GET_COUNT(pLayer_8, lBitfield_8, pMotorPos_32)
}

subcall modMotorOnMotor {
