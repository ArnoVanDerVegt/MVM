#project "Jump if greater demo"

#include "/lib/standard.asm"

proc main()
    number  n                               ; Declare a local number variable...

    set     n,                      0       ; Set n to 0
    cmp     n,                      1       ; Compare n to 1
    jg      myLabel                         ; jump to myLabel if n is greater than to 1
    print   "n <= 1"
myLabel:

endp