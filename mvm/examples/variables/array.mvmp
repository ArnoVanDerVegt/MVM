#project "Conditions"
    
#include "/lib/standard.mvm"
    
    number  list[10]
    number  counter
    number  value
    
proc main()
    set     counter,                0
loop:
    set     value,                  10
    sub     value,                  counter
    arrayw  list,                   counter, value
    
    inc     counter
    cmp     counter,                10
    jl      loop
    
    arrayr  value,                  list, 3
    log     value
endp