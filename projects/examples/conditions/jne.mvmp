#project "Jump if not equal demo"

#include "/lib/standard.asm"

proc main()
    number  n                               ; Declare a local number variable...

    set     n,                      2       ; Set n to 0
    cmp     n,                      1       ; Compare n to 1
    jne     myLabel                         ; jump to myLabel if n is not equal to 1
    print   "n == 1"
myLabel:

endp