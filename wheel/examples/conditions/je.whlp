#project "Jump if equal demo"

#include "/lib/standard.whl"

proc main()
    number  n                               ; Declare a local number variable...

    set     n,                      1       ; Set n to 1
    cmp     n,                      0       ; Compare n to 0
    je      myLabel                         ; jump to myLabel if n is equal to 0
    printS("n <> 0")
myLabel:

endp