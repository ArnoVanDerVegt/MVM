#project "Array of procedure pointers demo"

#include "/lib/standard.whl"

proc procList[2]

proc demoProc1()
    printS("Procedure 1")
endp

proc demoProc2()
    printS("Procedure 2")
endp

proc main()
    arrayw procList, 0, demoProc1
    arrayw procList, 1, demoProc2
	proc p
    arrayr p, procList, 1
    p()
    arrayr p, procList, 0
    p()
endp