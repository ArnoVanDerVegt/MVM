#project "Array record demo"

#include "/lib/standard.whl"

record Point
	number x, y, z
endr

Point srcPoint
Point points[3]
Point destPoint

proc main()
	set srcPoint.x, 4
    set srcPoint.y, 7
    set srcPoint.z, -10
    arrayw points, 1, srcPoint

    arrayr destPoint, points, 1
    printN(destPoint.x)
    printN(destPoint.y)
    printN(destPoint.z)
endp